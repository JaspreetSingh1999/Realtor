<body>
    <nav id="mainNavbar" class="navbar navbar-expand-lg navbar-dark bg-theme fontfull">
        <div class="container">
          <% if (!currentUser) { %>
            <img src="images/favicon-white.png" width="45" alt="" class="d-inline-block align-middle mr-2">
            
          <a class="navbar-brand fontfull" href="/">Realtor</a>
          <% } else { %>
            <img src="images/favicon-white.png" width="45" alt="" class="d-inline-block align-middle mr-2">
            <a class="navbar-brand fontfull" href="/home">Realtor</a>
          <% } %>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
            aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
              <li id="home" class="nav-item active">
                <a class="nav-link" href="/home">Home</a>
              </li>
              <li id="Properties" class="nav-item">
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true"
                    aria-expanded="false">
                    Properties
                  </a>
                  <div id="ddown1" class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
                    <a class="dropdown-item" href="/Properties/Commercial">Commercial</a>
                    <a class="dropdown-item" href="/Properties/Agriculture">Agriculture</a>
                    <a class="dropdown-item" href="/Properties/Residential">Residential</a>
                  </div>
                </li>
              </li>
              <li id="about" class="nav-item">
                <a class="nav-link" href="/about">About</a>
              </li>
              <li id="newCampground" class="nav-item">
                <a class="nav-link" href="/properties/new">New</a>
              </li>
            </ul>
            <ul class="navbar-nav ml-auto">
              <% if(!currentUser) { %>
                <li id="login" class="nav-item">
                  <a class="nav-link" href="/login?returnurl=home">Login</a>
                </li>
                <li id="register" class="nav-item">
                  <a class="nav-link" href="/register">Sign Up</a>
                </li>
              <% } else { %>
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true"
